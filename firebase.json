{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/prave{,/**}",
        "destination": "/#prave",
        "type": 301
      },
      {
        "source": "/fitster{,/**}",
        "destination": "/#fitster",
        "type": 301
      }
    ]
  }
}
